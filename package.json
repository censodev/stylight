{
  "name": "stylight-css",
  "version": "1.0.0",
  "description": "A mini utility scss library based on bootstrap from @censodev with love",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel css-compile* css-clean*",
    "css-compile": "node-sass stylight.scss stylight.css",
    "css-clean": "cleancss -o stylight.min.css stylight.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/censodev/stylight.git"
  },
  "publishConfig": { 
    "registry": "https://npm.pkg.github.com/" 
  },
  "author": "censodev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/censodev/stylight/issues"
  },
  "homepage": "https://github.com/censodev/stylight#readme",
  "dependencies": {
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  }
}
